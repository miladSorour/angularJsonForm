  "scripts": {
    "start-demo-app": "nx serve demo",
    "build-demo-app": "nx build demo --skip-nx-cache --verbose",
    "build-json-form-lib": "nx build json-form -skip-nx-cache --verbose",
    "build-metronic-lib": "nx build metronic-template -skip-nx-cache --verbose",
    "pack-json-form-lib": "cd dist/libs/json-form && npm pack && copy pnrng-json-form-1.0.0.tgz D:\\projects\\json-form-builder\\packs\\ && copy pnrng-json-form-1.0.0.tgz D:\\projects\\base-front\\dist\\libs\\json-form\\",
    "pack-metronic-lib": "cd dist/libs/metronic-template && npm pack && copy pnrng-metronic-template-1.0.0.tgz D:\\projects\\json-form-builder\\packs\\ && copy pnrng-metronic-template-1.0.0.tgz D:\\projects\\base-front\\dist\\libs\\metronic-template\\",
    "install-json-form-pack": "npm install dist/libs/json-form/pnrng-json-form-1.0.1.tgz --force --verbose",
    "install-metronic-pack": "npm install dist/libs/metronic-template/pnrng-metronic-template-1.0.0.tgz --force",
    "build-libs": "npm run build-json-form-lib && npm run build-metronic-lib",
    "pack-libs": "npm run pack-json-form-lib && npm run pack-metronic-lib",
    "install-packs": "npm run install-json-form-pack && npm run install-metronic-pack",
    "run-demo": "http-server dist/apps/demo/ -a 192.168.100.190 -p 4100 --cors -P http://192.168.100.184:8088/",
    "config-set": "npm config set registry=http://192.168.100.17:8081/repository/npm-group/ --location=project",
    "config-delete": "npm config delete registry --location=project",
    "watch": "ng build --watch --configuration development",
    "rtl": "webpack --config webpack.config.js",
    "clear-cache": "nx reset",
    "lint": "eslint apps//**//*.{js,jsx,ts,tsx}",
    "lint-fix": "npx eslint --cache --fix apps//**//*.{ts}",
    "prettier-check": "npx prettier \"{apps,libs}/**/*.{ts,scss}\" --loglevel warn --check --config ./.prettierrc",
    "prettier-write": "npx prettier \"{apps,libs}/**/*.{ts,scss}\" --loglevel warn--write --config ./.prettierrc"
  },
